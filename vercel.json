{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/v1/categories(.*)", "dest": "/index.js" },
    { "src": "/api/v1/subcategory(.*)", "dest": "/index.js" },
    { "src": "/api/v1/product(.*)", "dest": "/index.js" },
    { "src": "/api/v1/brand(.*)", "dest": "/index.js" },
    { "src": "/api/v1/user(.*)", "dest": "/index.js" },
    { "src": "/api/v1/auth(.*)", "dest": "/index.js" },
    { "src": "/api/v1/review(.*)", "dest": "/index.js" },
    { "src": "/api/v1/testimonial(.*)", "dest": "/index.js" },
    { "src": "/api/v1/wishlist(.*)", "dest": "/index.js" },
    { "src": "/api/v1/address(.*)", "dest": "/index.js" },
    { "src": "/api/v1/coupon(.*)", "dest": "/index.js" },
    { "src": "/api/v1/cart(.*)", "dest": "/index.js" },
    { "src": "/api/v1/order(.*)", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
